{
  "name": "peregrine-frontend",
  "private": true,
  "description": "A scouting app for FRC competitions. This is the frontend, written in TypeScript with Preact",
  "license": "MIT",
  "repository": "Pigmice2733/peregrine-frontend",
  "engines": {
    "node": "^12.18.2 || ^14.5.0"
  },
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "check-lint": "eslint --format=pretty --ignore-path .gitignore --ext .js,.ts,.tsx . && prettier --list-different --ignore-path .gitignore \"**/*.{j,t}s{x,}{on,}\"",
    "eslint": "eslint --format=pretty --ignore-path .gitignore --fix --ext .js,.ts,.tsx .",
    "lint": "eslint --format=pretty --ignore-path .gitignore --fix --ext .js,.ts,.tsx . && prettier --write --ignore-path .gitignore \"**/*.{j,t}s{x,}{on,}\"",
    "new-route": "hygen route new",
    "rollup-watch": "rollup -c rollup.config.js --watch",
    "start": "webpack-dev-server",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "type": "tsc",
    "type:watch": "tsc --watch",
    "webpack-build": "webpack"
  },
  "dependencies": {
    "ganalytics": "3.1.3",
    "matchit": "1.1.0",
    "polished": "4.0.3",
    "preact": "10.5.5",
    "qss": "2.0.3",
    "@mdi/js": "5.8.55"
  },
  "devDependencies": {
    "@babel/core": "7.12.9",
    "@babel/plugin-proposal-class-properties": "7.12.1",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.12.1",
    "@babel/plugin-proposal-numeric-separator": "7.12.7",
    "@babel/plugin-proposal-object-rest-spread": "7.12.1",
    "@babel/plugin-proposal-optional-chaining": "7.12.7",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-modules-commonjs": "7.12.1",
    "@babel/plugin-transform-react-jsx": "7.12.7",
    "@babel/plugin-transform-react-jsx-source": "7.12.1",
    "@babel/plugin-transform-template-literals": "7.12.1",
    "@babel/preset-env": "7.12.7",
    "@babel/preset-typescript": "7.12.7",
    "@calebeby/preact-testing-library": "0.0.4",
    "@rollup/plugin-babel": "5.2.1",
    "@rollup/plugin-node-resolve": "10.0.0",
    "@testing-library/jest-dom": "5.11.6",
    "@types/jest": "26.0.15",
    "@types/node-fetch": "2.5.7",
    "@types/sharp": "0.26.1",
    "babel-jest": "26.6.3",
    "babel-loader": "8.2.1",
    "babel-plugin-const-enum": "1.0.1",
    "babel-plugin-minify-dead-code-elimination": "0.5.1",
    "babel-plugin-module-resolver": "4.0.0",
    "babel-plugin-transform-inline-environment-variables": "0.4.3",
    "clsx": "1.1.1",
    "cpy": "8.1.1",
    "css-loader": "3.6.0",
    "dotenv": "8.2.0",
    "eslint": "7.14.0",
    "eslint-formatter-pretty": "4.0.0",
    "eslint-plugin-caleb": "11.0.0",
    "fake-indexeddb": "3.1.2",
    "friendly-errors-webpack-plugin": "1.7.0",
    "html-webpack-plugin": "4.5.0",
    "hygen": "1.6.2",
    "identity-obj-proxy": "3.0.0",
    "jest": "26.6.3",
    "jest-circus": "26.6.3",
    "linaria": "1.4.1",
    "linaria-preact": "1.3.2",
    "linaria-styled-preact": "1.1.2",
    "mini-css-extract-plugin": "0.12.0",
    "mkdirplz": "1.0.2",
    "node-fetch": "2.6.1",
    "postcss": "7.0.35",
    "postcss-calc": "7.0.5",
    "postcss-color-mod-function": "3.0.3",
    "postcss-css-variables": "0.17.0",
    "postcss-font-magician": "2.3.1",
    "postcss-import": "12.0.1",
    "postcss-loader": "3.0.0",
    "postcss-modules": "2.0.0",
    "postcss-preset-env": "6.7.0",
    "prettier": "2.1.2",
    "prompts": "1.2.1",
    "rollup": "2.33.3",
    "rollup-plugin-netlify-push": "1.0.0",
    "rollup-plugin-postcss": "3.1.8",
    "rollup-plugin-terser": "7.0.2",
    "sharp": "0.26.3",
    "templite": "1.1.0",
    "type-fest": "0.20.0",
    "typescript": "4.0.5",
    "webpack": "4.44.2",
    "webpack-bundle-analyzer": "3.9.0",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0",
    "webpackbar": "4.0.0"
  },
  "browserslist": [
    ">.5% in us",
    "not ie 11",
    "not ios_saf <11.1",
    "not edge <79",
    "not android <81"
  ],
  "eslintConfig": {
    "extends": [
      "plugin:caleb/recommended",
      "plugin:caleb/preact",
      "plugin:caleb/jest"
    ],
    "rules": {
      "no-return-await": "off",
      "caleb/react/display-name": "off",
      "no-else-return": [
        "error",
        {
          "allowElseIf": true
        }
      ],
      "caleb/@typescript-eslint/ban-ts-ignore": "off",
      "caleb/react/react-in-jsx-scope": "off",
      "no-restricted-imports": [
        "error",
        {
          "paths": [
            {
              "name": "preact",
              "importNames": [
                "h",
                "Fragment"
              ],
              "message": "The JSX runtime is injected automatically"
            }
          ]
        }
      ]
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "\\.css$": "identity-obj-proxy"
    },
    "transform": {
      "\\.ts$": "./jest-transform",
      "\\.tsx$": "./jest-transform"
    },
    "setupFilesAfterEnv": [
      "./jest.setup.ts"
    ],
    "testEnvironment": "jsdom",
    "testRunner": "jest-circus/runner",
    "testMatch": [
      "**/?(*.)+(spec|test).(j|t)s?(x)"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  }
}
