{
  "name": "peregrine-frontend",
  "private": true,
  "description": "A scouting app for FRC competitions. This is the frontend, written in TypeScript with Preact",
  "license": "MIT",
  "repository": "Pigmice2733/peregrine-frontend",
  "engines": {
    "node": "12.16.3"
  },
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "check-lint": "eslint --format=pretty --ignore-path .gitignore --ext .js,.ts,.tsx . && prettier --list-different --ignore-path .gitignore \"**/*.{j,t}s{x,}{on,}\"",
    "eslint": "eslint --format=pretty --ignore-path .gitignore --fix --ext .js,.ts,.tsx .",
    "lint": "eslint --format=pretty --ignore-path .gitignore --fix --ext .js,.ts,.tsx . && prettier --write --ignore-path .gitignore \"**/*.{j,t}s{x,}{on,}\"",
    "new-route": "hygen route new",
    "rollup-watch": "rollup -c rollup.config.js --watch",
    "start": "webpack-dev-server",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "type": "tsc",
    "type:watch": "tsc --watch",
    "webpack-build": "webpack"
  },
  "dependencies": {
    "ganalytics": "3.1.3",
    "matchit": "1.0.8",
    "polished": "3.6.2",
    "preact": "10.4.1",
    "qss": "2.0.3",
    "@mdi/js": "5.1.45"
  },
  "devDependencies": {
    "@babel/core": "7.9.6",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.8.3",
    "@babel/plugin-proposal-numeric-separator": "7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "7.9.6",
    "@babel/plugin-proposal-optional-chaining": "7.9.0",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-modules-commonjs": "7.9.6",
    "@babel/plugin-transform-react-jsx": "7.9.4",
    "@babel/plugin-transform-react-jsx-source": "7.9.0",
    "@babel/plugin-transform-template-literals": "7.8.3",
    "@babel/preset-modules": "0.1.3",
    "@babel/preset-typescript": "7.9.0",
    "@calebeby/preact-testing-library": "0.0.4",
    "@rollup/plugin-node-resolve": "8.0.0",
    "@testing-library/jest-dom": "5.8.0",
    "@types/jest": "25.2.3",
    "@types/node-fetch": "2.5.7",
    "@types/sharp": "0.25.0",
    "babel-jest": "26.0.1",
    "babel-loader": "8.1.0",
    "babel-plugin-const-enum": "1.0.1",
    "babel-plugin-minify-dead-code-elimination": "0.5.1",
    "babel-plugin-module-resolver": "4.0.0",
    "babel-plugin-transform-inline-environment-variables": "0.4.3",
    "clsx": "1.1.0",
    "cpy": "8.1.0",
    "css-loader": "3.5.3",
    "dotenv": "8.2.0",
    "eslint": "7.1.0",
    "eslint-formatter-pretty": "3.0.1",
    "eslint-plugin-caleb": "9.0.0",
    "fake-indexeddb": "3.0.1",
    "friendly-errors-webpack-plugin": "1.7.0",
    "html-webpack-plugin": "3.2.0",
    "hygen": "1.6.2",
    "identity-obj-proxy": "3.0.0",
    "jest": "26.0.1",
    "jest-circus": "26.0.1",
    "linaria": "1.3.3",
    "linaria-preact": "1.3.2",
    "linaria-styled-preact": "1.1.2",
    "mini-css-extract-plugin": "0.9.0",
    "mkdirplz": "1.0.2",
    "node-fetch": "2.6.0",
    "postcss": "7.0.31",
    "postcss-calc": "7.0.2",
    "postcss-color-mod-function": "3.0.3",
    "postcss-css-variables": "0.17.0",
    "postcss-font-magician": "2.3.1",
    "postcss-import": "12.0.1",
    "postcss-loader": "3.0.0",
    "postcss-modules": "2.0.0",
    "postcss-preset-env": "6.7.0",
    "prettier": "2.0.5",
    "prompts": "1.2.1",
    "rollup": "1.32.1",
    "rollup-plugin-babel": "4.4.0",
    "rollup-plugin-netlify-push": "0.2.0",
    "rollup-plugin-postcss": "2.9.0",
    "rollup-plugin-terser": "5.3.0",
    "sharp": "0.25.3",
    "templite": "1.1.0",
    "type-fest": "0.15.0",
    "typescript": "3.8.3",
    "webpack": "4.43.0",
    "webpack-bundle-analyzer": "3.8.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.11.0",
    "webpackbar": "4.0.0"
  },
  "browserslist": [
    ">1% in my stats"
  ],
  "eslintConfig": {
    "extends": [
      "plugin:caleb/recommended",
      "plugin:caleb/preact",
      "plugin:caleb/jest"
    ],
    "rules": {
      "no-return-await": "off",
      "caleb/react/display-name": "off",
      "no-else-return": [
        "error",
        {
          "allowElseIf": true
        }
      ],
      "caleb/@typescript-eslint/ban-ts-ignore": "off"
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "\\.css$": "identity-obj-proxy"
    },
    "transform": {
      "\\.ts$": "./jest-transform",
      "\\.tsx$": "./jest-transform"
    },
    "setupFilesAfterEnv": [
      "./jest.setup.ts"
    ],
    "testEnvironment": "jsdom",
    "testRunner": "jest-circus/runner",
    "testMatch": [
      "**/?(*.)+(spec|test).(j|t)s?(x)"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  }
}
