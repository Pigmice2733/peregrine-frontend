{
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "description": "A scouting app for FRC competitions. This is the frontend, written in TypeScript with Preact",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "check-lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx . && prettier --list-different --ignore-path .gitignore \"**/*.{j,t}s{x,}\"",
    "lint": "eslint --ignore-path .gitignore --fix --ext .js,.ts,.tsx . && prettier --write --ignore-path .gitignore \"**/*.{j,t}s{x,}\"",
    "type:watch": "tsc --watch",
    "type": "tsc",
    "start": "parcel index.html",
    "build": "node build.js",
    "new-route": "hygen route new"
  },
  "eslintConfig": {
    "extends": [
      "plugin:caleb/recommended",
      "plugin:caleb/preact",
      "plugin:caleb/jest"
    ],
    "rules": {
      "caleb/react/display-name": "off"
    }
  },
  "dependencies": {
    "preact": "8.4.2",
    "preact-router": "2.6.1",
    "systemjs": "2.1.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "\\.css$": "identity-obj-proxy"
    },
    "transform": {
      "\\.ts$": "./jest-transform",
      "\\.tsx$": "./jest-transform"
    },
    "testMatch": [
      "**/?(*.)+(spec|test).(j|t)s?(x)"
    ]
  },
  "browserslist": [
    ">3%"
  ],
  "devDependencies": {
    "@babel/core": "7.2.0",
    "@babel/plugin-proposal-class-properties": "7.2.1",
    "@babel/plugin-proposal-object-rest-spread": "7.2.0",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-transform-template-literals": "7.2.0",
    "@babel/preset-typescript": "7.1.0",
    "@types/jest": "23.3.10",
    "@types/node-fetch": "2.1.4",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "23.6.0",
    "babel-plugin-module-resolver": "3.1.1",
    "babel-plugin-transform-inline-environment-variables": "0.4.3",
    "cpy": "7.0.1",
    "eslint": "5.10.0",
    "eslint-plugin-caleb": "1.13.0",
    "hygen": "1.6.2",
    "identity-obj-proxy": "3.0.0",
    "jest": "23.6.0",
    "kleur": "3.0.1",
    "node-fetch": "2.3.0",
    "parcel-bundler": "1.10.3",
    "postcss": "7.0.6",
    "postcss-calc": "7.0.1",
    "postcss-color-mod-function": "3.0.3",
    "postcss-css-variables": "0.11.0",
    "postcss-font-magician": "2.2.1",
    "postcss-import": "12.0.1",
    "postcss-modules": "1.4.1",
    "postcss-preset-env": "6.4.0",
    "preact-testing-library": "0.3.0",
    "prettier": "1.15.3",
    "prompts": "1.2.1",
    "rollup": "0.67.4",
    "rollup-plugin-babel": "4.0.3",
    "rollup-plugin-node-resolve": "3.4.0",
    "rollup-plugin-postcss": "1.6.3",
    "rollup-plugin-terser": "3.0.0",
    "templite": "1.1.0",
    "typescript": "3.2.2"
  },
  "engines": {
    "node": "10"
  }
}
