{
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "repository": "Pigmice2733/peregrine-frontend",
  "description": "A scouting app for FRC competitions. This is the frontend, written in TypeScript with Preact",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "check-lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx . && prettier --list-different --ignore-path .gitignore \"**/*.{j,t}s{x,}\"",
    "lint": "eslint --ignore-path .gitignore --fix --ext .js,.ts,.tsx . && prettier --write --ignore-path .gitignore \"**/*.{j,t}s{x,}\"",
    "type:watch": "tsc --watch",
    "type": "tsc",
    "start": "webpack-dev-server",
    "build": "node build.js",
    "new-route": "hygen route new"
  },
  "eslintConfig": {
    "extends": [
      "plugin:caleb/recommended",
      "plugin:caleb/preact",
      "plugin:caleb/jest"
    ],
    "rules": {
      "caleb/react/display-name": "off",
      "no-else-return": [
        "error",
        {
          "allowElseIf": true
        }
      ]
    }
  },
  "dependencies": {
    "matchit": "^1.0.8",
    "polished": "^3.3.0",
    "preact": "^10.0.0-beta.1",
    "systemjs": "3.1.6"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "\\.css$": "identity-obj-proxy"
    },
    "transform": {
      "\\.ts$": "./jest-transform",
      "\\.tsx$": "./jest-transform"
    },
    "setupFilesAfterEnv": [
      "./jest.setup.ts"
    ],
    "testMatch": [
      "**/?(*.)+(spec|test).(j|t)s?(x)"
    ]
  },
  "browserslist": [
    ">3%"
  ],
  "devDependencies": {
    "@babel/core": "7.4.4",
    "@babel/plugin-proposal-class-properties": "7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "7.4.4",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-transform-modules-commonjs": "^7.4.3",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/plugin-transform-template-literals": "7.4.4",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-typescript": "7.3.3",
    "@types/jest": "24.0.5",
    "@types/node-fetch": "2.3.4",
    "babel-jest": "24.8.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-minify-dead-code-elimination": "^0.5.0",
    "babel-plugin-module-resolver": "3.2.0",
    "babel-plugin-transform-inline-environment-variables": "0.4.3",
    "clsx": "1.0.4",
    "cpy": "7.2.0",
    "css-loader": "^2.1.1",
    "eslint": "5.16.0",
    "eslint-plugin-caleb": "4.0.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "hygen": "1.6.2",
    "identity-obj-proxy": "3.0.0",
    "jest": "24.1.0",
    "linaria": "^1.3.1",
    "linaria-preact": "^1.3.2",
    "linaria-styled-preact": "^1.1.2",
    "mini-css-extract-plugin": "^0.6.0",
    "node-fetch": "2.6.0",
    "postcss": "7.0.16",
    "postcss-calc": "7.0.1",
    "postcss-color-mod-function": "3.0.3",
    "postcss-css-variables": "0.12.0",
    "postcss-font-magician": "2.2.1",
    "postcss-import": "12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-modules": "1.4.1",
    "postcss-preset-env": "6.6.0",
    "preact-testing-library": "0.3.0",
    "prettier": "1.17.1",
    "prompts": "1.2.1",
    "rollup": "1.12.2",
    "rollup-plugin-babel": "4.3.2",
    "rollup-plugin-node-resolve": "5.0.0",
    "rollup-plugin-postcss": "2.0.3",
    "rollup-plugin-terser": "4.0.4",
    "templite": "1.1.0",
    "typescript": "3.4.5",
    "webpack": "^4.30.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.1",
    "webpack-dev-server": "^3.3.1",
    "webpackbar": "^3.2.0"
  },
  "engines": {
    "node": "12",
    "npm": ">=6.5.0"
  }
}
